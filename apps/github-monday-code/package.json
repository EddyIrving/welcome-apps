{
  "name": "monday-integration-github",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "start": "npm run db:migrate && npm run server",
    "server": "nodemon ./src/app.js",
    "db:migrate": "sequelize db:migrate",
    "deploy": "mapps code:push --verbose",
    "logs": "mapps code:logs",
    "mapps:init": "mapps init"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^0.1.1",
    "@octokit/rest": "^16.43.2",
    "body-parser": "^1.19.0",
    "cross-port-killer": "^1.2.1",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "monday-sdk-js": "^0.1.4",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "sequelize": "^6.30.0",
    "simple-oauth2": "^4.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^0.1.13",
    "@mondaydotcomorg/tunnel": "^0.4.2",
    "concurrently": "^5.2.0",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^6.2.0"
  }
